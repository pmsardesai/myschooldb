{
  	"extends": "airbnb",

  	"parser": "babel-eslint",

    "plugins": ["react"],

  	"ecmaFeatures": {
  		"jsx": true,
      "classes": true
  	},

  	"env": {
  		"es6": true
  	},

  	"rules": {
	    "comma-dangle": 0,
      "react/jsx-quotes": 0,
      "jsx-quotes": 0,
      "quotes": 0,
      "indent": 0,
      "no-trailing-spaces": 0,
      "eol-last": 0,
      "prefer-const": 0,
      "new-cap": 0,
      "spaced-comment": 0,
      "react/sort-comp": 0,
      "react/no-unknown-property": 0,
      "no-mixed-spaces-and-tabs": 0
  	}
}